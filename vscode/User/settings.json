// Place your settings in this file to overwrite the default settings
{
    "editor.formatOnSave": true,
    "editor.tabSize": 4,
    "editor.detectIndentation": false,
    "editor.insertSpaces": true,
    "editor.wordWrap": "bounded",
    "editor.wrappingIndent": "indent",
    "editor.wordWrapColumn": 120,
    "editor.rulers": [120],
    "editor.mouseWheelZoom": true,
    "editor.renderIndentGuides": false,
    "editor.minimap.enabled": false,
    "editor.renderLineHighlight": "all",
    "editor.cursorStyle": "line",
    "editor.cursorBlinking": "blink",
    "window.zoomLevel": 0,
    "window.openFoldersInNewWindow": "default",
    "explorer.openEditors.visible": 9,
    "explorer.autoReveal": true,
    "terminal.external.linuxExec": "konsole",
    "workbench.colorTheme": "Monokai",
    "workbench.iconTheme": "material-theme-icons",
    "update.channel": "default",
    "extensions.autoUpdate": true,
    "eslint.enable": true,
    "stylelint.enable": true,
    "git.autofetch": false,
    "css.validate": true,
    "go.useLanguageServer": false,
    "go.gotoSymbol.includeImports": false,
    "lldb.executable": "lldb",
    "rust.showOutput": false,
    "rust.actionOnSave": null,
    "rust.cargoHomePath": "/home/oasf/.cargo",
    "rust.rustLangSrcPath": "/home/oasf/.rustup/toolchains/nightly-x86_64-unknown-linux-gnu/lib/rustlib/src/rust/src",
    "rust.executeCargoCommandInTerminal": true,
    "rust.actionOnStartingCommandIfThereIsRunningCommand": "Stop running command",
    "rust.rls": {
        "executable": "rustup",
        "args": ["run", "nightly", "rls"],
        "revealOutputChannelOn": "warn"
    },
    "files.associations": {
        ".*": "shellscript"
    }
}