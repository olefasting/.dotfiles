// Place your settings in this file to overwrite the default settings
{
    "editor.formatOnSave": true,
    "editor.tabSize": 4,
    "editor.detectIndentation": false,
    "editor.insertSpaces": true,
    "editor.wordWrap": "bounded",
    "editor.wrappingIndent": "indent",
    "editor.wordWrapColumn": 160,
    "editor.rulers": [160],
    "editor.mouseWheelZoom": true,
    "editor.renderIndentGuides": true,
    "editor.minimap.enabled": true,
    "editor.renderLineHighlight": "all",
    "editor.cursorStyle": "line",
    "editor.cursorBlinking": "blink",
    "window.zoomLevel": 0,
    "window.openFoldersInNewWindow": "default",
    "explorer.openEditors.visible": 9,
    "explorer.autoReveal": true,
    "workbench.colorTheme": "Darktooth",
    "workbench.iconTheme": "material-theme-icons",
    "extensions.autoUpdate": true,
    "eslint.enable": true,
    "stylelint.enable": true,
    "git.autofetch": false,
    "css.validate": true,
    "go.useLanguageServer": false,
    "go.gotoSymbol.includeImports": false,
    "rust.actionOnSave": "clippy",
    "rust.showOutput": true,
    "rust.executeCargoCommandInTerminal": true,
    "rust.actionOnStartingCommandIfThereIsRunningCommand": "Stop running command",
    "lldb.executable": "lldb",
    "[plaintext]": {

    }
}