{
  "editor.tabSize": 4,
  "editor.insertSpaces": true,
  "editor.wordWrapColumn": 120,
  "editor.wordWrap": "bounded",
  "files.associations": {
    ".*": "shellscript",
    ".gitignore": "gitignore"
  },
  "[json]": {
    "editor.tabSize": 2
  },
  "[html]": {
    "editor.tabSize": 2,
    "editor.wordWrapColumn": 200,
    "editor.wordWrap": "on"
  }
}
